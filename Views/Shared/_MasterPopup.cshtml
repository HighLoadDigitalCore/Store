@{
    AccessHelper.Repository.Clear();
}
<!DOCTYPE html>
<html lang="ru">
<head>
    <meta charset="utf-8">
    <title>@ViewBag.Title</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0, minimum-scale=1.0, maximum-scale=1.0">
    @if (SiteSetting.Get<string>("FavIcon").IsFilled())
    {
        <link rel="SHORTCUT ICON" href="@(SiteSetting.Get<string>("FavIcon"))">
    }
    else
    {
        <link rel="shortcut icon" href="/content/photon/favicon.ico" />
    }
    <link rel="stylesheet" href="/content/photon/css/css/photon.css" media="all" />
    <link rel="stylesheet" href="/content/photon/css/css/photon-pt2.css" media="all" />

    <script type="text/javascript" src="/Scripts/jquery-1.8.1.min.js"></script>
    <script type="text/javascript" src="/Scripts/jquery-ui-1.10.3.min.js"></script>
    
    <script type="text/javascript" src="/content/photon/js/bootstrap/bootstrap.min.js"></script>
    <script type="text/javascript" src="/content/photon/js/plugins/modernizr.custom.js"></script>
    <script type="text/javascript" src="/content/photon/js/plugins/jquery.pnotify.min.js"></script>
    <script type="text/javascript" src="/content/photon/js/plugins/less-1.3.1.min.js"></script>
    <script type="text/javascript" src="/content/photon/js/plugins/xbreadcrumbs.js"></script>
    <script type="text/javascript" src="/content/photon/js/plugins/jquery.maskedinput-1.3.min.js"></script>
    <script type="text/javascript" src="/content/photon/js/plugins/jquery.autotab-1.1b.js"></script>
    <script type="text/javascript" src="/content/photon/js/plugins/charCount.js"></script>
    <script type="text/javascript" src="/content/photon/js/plugins/jquery.textareaCounter.js"></script>
    <script type="text/javascript" src="/content/photon/js/plugins/elrte.min.js"></script>
    <script type="text/javascript" src="/content/photon/js/plugins/elrte.en.js"></script>
    <script type="text/javascript" src="/content/photon/js/plugins/select2.js"></script>
    <script type="text/javascript" src="/content/photon/js/plugins/jquery-picklist.min.js"></script>
    <script type="text/javascript" src="/content/photon/js/plugins/jquery.form.js"></script>
    <script type="text/javascript" src="/content/photon/js/plugins/jquery.metadata.js"></script>
    <script type="text/javascript" src="/content/photon/js/plugins/jquery.mockjax.js"></script>
    <script type="text/javascript" src="/content/photon/js/plugins/jquery.uniform.min.js"></script>
    <script type="text/javascript" src="/content/photon/js/plugins/jquery.tagsinput.min.js"></script>
    <script type="text/javascript" src="/content/photon/js/plugins/jquery.rating.pack.js"></script>
    <script type="text/javascript" src="/content/photon/js/plugins/jquery.timeentry.min.js"></script>
    <script type="text/javascript" src="/content/photon/js/plugins/jquery.jstree.js"></script>
    <script type="text/javascript" src="/content/photon/js/plugins/jquery.mousewheel.min.js"></script>
    <script type="text/javascript" src="/content/photon/js/plugins/jquery.mCustomScrollbar.js"></script>
    <script type="text/javascript" src="/content/photon/js/plugins/jquery.jqtweet.js"></script>
    <script type="text/javascript" src="/content/photon/js/plugins/jquery.cookie.js"></script>
    <script type="text/javascript" src="/content/photon/js/plugins/bootstrap-fileupload.min.js"></script>
    <script type="text/javascript" src="/content/photon/js/plugins/prettify/prettify.js"></script>
    <script type="text/javascript" src="/content/photon/js/plugins/bootstrapSwitch.js"></script>
    <script type="text/javascript" src="/content/photon/js/plugins/mfupload.js"></script>

    <script type="text/javascript" src="/content/photon/js/common.js"></script>


    <script type="text/javascript" src="/Scripts/jquery-ui-timepicker-addon.js"></script>
    <script type="text/javascript" src="/Scripts/modernizr-2.6.2.js"></script>


    <script type="text/javascript" src="/content/photon/js/plugins/jquery.validate.min.js"></script>
    <script type="text/javascript" src="/Scripts/jquery.validate.unobtrusive.min.js"></script>
    <script type="text/javascript" src="/content/photon/js/plugins/additional-methods.min.js"></script>
    <script type="text/javascript" src="/Scripts/jquery.unobtrusive-ajax.min.js"></script>
    <script type="text/javascript" src="/Scripts/FileUpload/jquery.fileupload.js"></script>
    <script type="text/javascript" src="/Scripts/FileUpload/jquery.fileuploadui.js"></script>
    <script type="text/javascript" src="/Scripts/FileUpload/jquery.iframe-transport.js"></script>
    <script type="text/javascript" src="/Scripts/FileUpload/jquery.upload-init.js"></script>
    
    <script type="text/javascript" src="/Scripts/master.js"></script>
    <script type="text/javascript" src="/Scripts/group-toggler.js"></script>
      
    <script type="text/javascript" src="~/Scripts/tinymce/tinymce.min.js"></script>
    
    @RenderSection("scripts", required: false)
</head>

<body style="min-width: 100%;" class="popup-body">
    <div style="display: none">
        @RenderSection("LeftTab", false)
        @RenderSection("Preview", false)
        @RenderSection("Description", false)
        @RenderSection("Header", false)
    </div>
<table class="container-tbl">
    <tr>
 
        <td class="content">
            <div class="content1">
             
                <table width="100%">
                    <tr>
                        <td class="content2-l" style="padding: 10px 10px;">
                            <div id="PageContent">
                                @RenderBody()
                            </div>
                        </td>
                    </tr>
                </table>
            </div>
        
        </td>
    </tr>
</table>
</body>
</html>
