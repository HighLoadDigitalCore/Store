    @RenderSection("scripts", required: false)
    <script type="text/javascript">
        $().ready(function() {
            calcSize();
        });

        function calcSize() {
            //debugger;
            var w = $(parent.document).find('.content2-l').width() + 'px';
            $('#body').css('width', w);
            $(parent.document).find('iframe[src="' + document.location.pathname + document.location.search + '"]').attr('width', w);

            var h = $(document).find('#innerBody').height() + 'px';
            $('#body').css('height', h);
            $(parent.document).find('iframe[src="' + document.location.pathname + document.location.search + '"]').attr('height', h);

        }
    </script>
